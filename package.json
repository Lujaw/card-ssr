{
  "name": "hcard-builder",
  "version": "1.0.0",
  "description": "hcard builder",
  "main": "server/index.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^26.0.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.3",
    "prop-types": "^15.7.2",
    "react": "^15.5.4",
    "react-bootstrap-table": "^4.3.1",
    "react-dom": "^15.5.4",
    "react-router-dom": "^5.1.2",
    "sequelize": "^5.21.7",
    "serialize-javascript": "^3.0.0",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.19.0",
    "jsdoc": "^3.6.4",
    "json-loader": "^0.5.7",
    "nock": "^12.0.3",
    "sequelize-cli": "^5.5.1",
    "sequelize-mock": "^0.10.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "jest --config=jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "init:db": "npx sequelize db:migrate",
    "build:dev": "webpack --mode=development --progress --colors",
    "build:dist": "webpack --mode=production -p --progress --colors",
    "serve": "nodemon dist",
    "docs": "npx jsdoc -r ./src/server",
    "prestart": "npm run build:dev",
    "start": "npx concurrently \"webpack --mode=development --watch\" \"npm run serve \"",
    "start:dist": "export PORT=3000 && npm run build:dist && npm run serve"
  },
  "keywords": [
    "hcard",
    "express",
    "react",
    "ssr"
  ],
  "author": "Luja Shrestha",
  "license": "ISC"
}